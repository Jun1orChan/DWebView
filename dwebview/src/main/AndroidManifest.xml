<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.jun1or.dwebview">

    <application>
        <receiver
            android:name="org.jun1or.dwebview.fragment.WebViewDownloadCompleteReceiver">
            <intent-filter>
                <action android:name="android.intent.action.DOWNLOAD_COMPLETE"/>
            </intent-filter>
        </receiver>
        <meta-data
            android:name="android.webkit.WebView.EnableSafeBrowing"
            android:value="true"/>
    </application>
</manifest>
